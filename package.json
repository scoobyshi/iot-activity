{
  "name": "iot-activity",
  "version": "0.0.1",
  "description": "Activity Monitor for IoT",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=integration jest --forceExit --verbose --detectOpenHandles --forceExit",
    "test:coverage": "npm test -- --coverage",
    "start": "node dist/index.js",
    "build": "tsc",
    "lint": "tslint --project tsconfig.json",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'src/index.ts' --exec 'ts-node --files'",
    "dev": "ts-node --files src/index.ts"
  },
  "author": "Scott Justason",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scoobyshi/iot-activity.git"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.23",
    "@types/pubnub": "^4.0.5",
    "@types/request": "^2.48.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.1.0",
    "jest-html-reporter": "^2.4.4",
    "nodemon": "^1.19.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-airbnb-base": "^0.2.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "aws-sdk": "^2.400.0",
    "moment": "^2.29.4",
    "pubnub": "^4.21.7",
    "uuid": "^3.3.2",
    "wink-api": "github:scoobyshi/wink-api#fix-declaration",
    "winston": "^3.2.1"
  }
